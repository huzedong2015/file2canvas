{
    /* file exclude */
    "files.exclude": {
        ".sass-cache": true,
        "node_modules": true
    },
    /* vs code */
    "editor.formatOnSave": true, /* 保存自动格式化 */
    "editor.formatOnType": false, /* 换行后自动格式化 */
    "editor.codeActionsOnSave": { /* 保存后自动修复 */
        "source.fixAll": true
    },
    /* eslint */
    "eslint.format.enable": false /* eslint格式化 */
}